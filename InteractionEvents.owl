  <owl:Class rdf:ID="SemanticTransformer">
    <owl:disjointWith>
      <owl:Class rdf:about="#SmartObject"/>
    </owl:disjointWith>
    <spin:rule>
      <sp:Construct>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate rdf:resource="http://sofia.gotdns.com/ontologies/SemanticConnections.owl#connectedTo"/>
            <sp:subject>
              <rdf:Description rdf:nodeID="A4">
                <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >source</sp:varName>
              </rdf:Description>
            </sp:subject>
          </rdf:Description>
          <rdf:Description>
            <sp:object>
              <rdf:Description rdf:nodeID="A5">
                <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >sink</sp:varName>
              </rdf:Description>
            </sp:object>
            <sp:predicate rdf:resource="http://sofia.gotdns.com/ontologies/SemanticConnections.owl#connectedTo"/>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
        </sp:templates>
        <sp:where rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate>
              <owl:IrreflexiveProperty rdf:about="http://sofia.gotdns.com/ontologies/SemanticConnections.owl#canConnectTo">
                <owl:propertyChainAxiom rdf:parseType="Collection">
                  <owl:ObjectProperty rdf:ID="functionalitySource"/>
                  <owl:ObjectProperty rdf:ID="isFunctionalityOfSink"/>
                </owl:propertyChainAxiom>
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
              </owl:IrreflexiveProperty>
            </sp:predicate>
            <sp:subject rdf:nodeID="A4"/>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:nodeID="A5"/>
            <sp:predicate rdf:resource="http://sofia.gotdns.com/ontologies/SemanticConnections.owl#canConnectTo"/>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:nodeID="A5"/>
            <sp:predicate rdf:resource="http://sofia.gotdns.com/ontologies/SemanticConnections.owl#connectedTo"/>
            <sp:subject rdf:nodeID="A4"/>
          </rdf:Description>
        </sp:where>
      </sp:Construct>
    </spin:rule>
