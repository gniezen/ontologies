            <sp:where rdf:parseType="Collection">
              <rdf:Description>
                <sp:object rdf:nodeID="A0"/>
                <sp:predicate rdf:resource="http://www.w3.org/2002/07/owl#hasValue"/>
                <sp:subject>
                  <rdf:Description rdf:nodeID="A3">
